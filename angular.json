{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": true,
      "environment": "local",
      "path": "./.cache"
    },
    "packageManager": "npm",
    "warnings": {
      "versionMismatch": true
    }
  },
  "newProjectRoot": "./e2e/",
  "projects": {
    "with-circular-dependencies": {
      "architect": {
        "lint-circular-deps": {
          "builder": "./dist:circular-dependencies-linter",
          "options": {
            "tsConfig": "./tsconfig.test-e2e.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "./e2e/with-circular-dependencies",
      "sourceRoot": "./e2e/with-circular-dependencies/src"
    }
  },
  "schematics": {
    "@schematics/angular:application": {
      "strict": true
    }
  },
  "version": 1
}
